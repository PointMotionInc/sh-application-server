{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../..",
  "testMatch": ["<rootDir>/test/e2e/*.e2e-spec.{ts,js}"],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["src/**/*.(t|j)s"],
  "coverageDirectory": "./coverage-e2e",
  "testEnvironment": "node",
  "moduleDirectories": ["node_modules", "<rootDir>"]
}
